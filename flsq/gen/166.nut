
/*
        Status generated by radio
*/
return class extends MAV.Message {
    constructor() {
        base.constructor()
    }
    static classname = "RADIO"
    static crcExtra = 21
    static fieldNames = ["rssi", "remrssi", "txbuf", "noise", "remnoise", "rxerrors", "fixed"]

    rssi = 0
    remrssi = 0
    txbuf = 0
    noise = 0
    remnoise = 0
    rxerrors = 0
    fixed = 0

    function packPayload(b) {
        writea(b, rxerrors, 'w', 0)
        writea(b, fixed, 'w', 0)
        writea(b, rssi, 'b', 0)
        writea(b, remrssi, 'b', 0)
        writea(b, txbuf, 'b', 0)
        writea(b, noise, 'b', 0)
        writea(b, remnoise, 'b', 0)
    }
    function unpackPayload(b) {
        rxerrors = reada(b, 'w', 0)
        fixed = reada(b, 'w', 0)
        rssi = reada(b, 'b', 0)
        remrssi = reada(b, 'b', 0)
        txbuf = reada(b, 'b', 0)
        noise = reada(b, 'b', 0)
        remnoise = reada(b, 'b', 0)
    }
}

